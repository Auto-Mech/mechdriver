
input
    run_prefix = ./RUN
    save_prefix = ./SAVE
end input

pes
    1: 1
end pes

spc
    5
end spc

els
    # Thermo Calculations
    spc init_geom   runlvl=wbs  inplvl=wbs
    spc conf_samp   runlvl=wbs  inplvl=wbs
    spc conf_hess   runlvl=wbs  inplvl=wbs  cnf_range=n100
    spc conf_opt    runlvl=b2t  inplvl=wbs  cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
    spc conf_hess   runlvl=b2t  inplvl=b2t  cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
    spc hr_scan     runlvl=b2t  inplvl=b2t  tors_model=1dhrf cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
    spc conf_energy runlvl=f12d inplvl=b2t  cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
    # Kinetics Calculations
    ts find_ts      runlvl=wbs  inplvl=wbs
    ts conf_samp    runlvl=wbs  inplvl=wbs
    ts conf_hess    runlvl=wbs  inplvl=wbs  cnf_range=n100
    ts conf_opt     runlvl=b2t  inplvl=wbs  cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
    ts conf_hess    runlvl=b2t  inplvl=b2t  cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
    ts hr_scan      runlvl=b2t  inplvl=b2t  tors_model=1dhrf
    ts conf_energy  runlvl=f12d inplvl=b2t  cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
end els

thermo
    write_mess    kin_model=global  spc_model=global cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
    run_mess      nprocs=10  kin_model=global  spc_model=global cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
    run_fits      kin_model=global  spc_model=global cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
end thermo

ktp
    write_mess    kin_model=global  spc_model=global cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
    run_mess      nprocs=10  kin_model=global  spc_model=global cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
    run_fits      kin_model=global  spc_model=global cnf_range=min sort=freqs(wbs),sp(wbs),gibbs(700)
